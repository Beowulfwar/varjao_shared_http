{
  "name": "@varjao/shared-http",
  "version": "0.1.0",
  "description": "Utilitários HTTP puros para frontends Varjão (base URL, headers, refresh dedupe)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./baseUrl": {
      "import": "./dist/baseUrl.js",
      "types": "./dist/baseUrl.d.ts"
    },
    "./headers": {
      "import": "./dist/headers.js",
      "types": "./dist/headers.d.ts"
    },
    "./authState": {
      "import": "./dist/authState.js",
      "types": "./dist/authState.d.ts"
    },
    "./refreshCoordinator": {
      "import": "./dist/refreshCoordinator.js",
      "types": "./dist/refreshCoordinator.d.ts"
    },
    "./errorPredicates": {
      "import": "./dist/errorPredicates.js",
      "types": "./dist/errorPredicates.d.ts"
    },
    "./deviceContext": {
      "import": "./dist/deviceContext.js",
      "types": "./dist/deviceContext.d.ts"
    },
    "./tokenManager": {
      "import": "./dist/tokenManager.js",
      "types": "./dist/tokenManager.d.ts"
    },
    "./clientFactory": {
      "import": "./dist/clientFactory.js",
      "types": "./dist/clientFactory.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "typecheck": "tsc -p tsconfig.typecheck.json"
  },
  "keywords": [
    "http",
    "refresh",
    "headers",
    "baseurl",
    "varjao"
  ],
  "author": "Varjão",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
